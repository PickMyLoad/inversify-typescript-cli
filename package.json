{
  "name": "inversify-typescript-cli",
  "version": "0.0.2",
  "description": "CLI for creating, editing and modifying an application using inversify dependency injection.",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "repository": "git@github.com:danimbrogno/inversify-cli.git",
  "author": "PML Freight Tech",
  "contributors": [
    "Dan Imbrogno <dan.imbrogno@gmail.com>"
  ],
  "license": "MIT",
  "private": false,
  "bin": "index.js",
  "scripts": {
    "build": "yarn run test && yarn run lint && yarn run clean && yarn run compile && yarn run e2e",
    "clean": "rimraf ./dist",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "compile": "tsc",
    "test": "ts-mocha --inspect=0.0.0.0:5858 src/**/*.spec.ts",
    "e2e": "ts-mocha --inspect=0.0.0.0:5858 e2e/**/*.e2e.ts",
    "debug": "ts-node --inspect=0.0.0.0:5858 ./src/index.ts",
    "ts-node": "ts-node",
    "ts-mocha": "ts-mocha",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "change-case": "^3.0.1",
    "rimraf": "^2.6.2",
    "ts-simple-ast": "^6.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/chai-string": "^1.1.31",
    "@types/fs-extra": "^4.0.3",
    "@types/lodash": "^4.14.77",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.45",
    "@types/yargs": "^8.0.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "fs-extra": "^4.0.2",
    "inversify": "^4.3.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.12.1",
    "reflect-metadata": "^0.1.10",
    "testdouble": "^3.2.6",
    "ts-mocha": "^1.0.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "yargs": "^10.0.3"
  },
  "peerDependencies": {
    "@types/mocha": "^2.2.46",
    "inversify": "^4.9.0",
    "mocha": "^4.1.0"
  }
}
